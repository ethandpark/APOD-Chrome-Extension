{
    "manifest_version": 2,
    "name": "APOD",
    "description": "change homepage of new tab to APOD from NASA",
    "version": "1.0",
    "browser_action": {
        "default_icon": "images/astronomy.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
    }],
    "permissions": [
        "tabs"
    ]

}