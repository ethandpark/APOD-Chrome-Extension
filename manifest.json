{
    "manifest_version": 2,
    "name": "APOD",
    "description": "change homepage of new tab to APOD from NASA",
    "version": "1.0",
    "browser_action": {
        "default_icon": "images/astronomy.png",
        "default_popup": "popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "apod.html"
    },
    "permissions": [
        "tabs"
    ]

}